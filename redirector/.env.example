# Redis password (REQUIRED for production)
REDIS_URL=rediss://default:<YOUR_PASSWORD>@<YOUR_REDIS_HOST>:<YOUR_REDIS_PORT>/0

# Comma-separated list of domains that bypass Redis check for TLS cert issuance
# Useful for infrastructure domains (e.g., redirector.rediredge.app)
WHITELIST_DOMAINS=redirector.rediredge.app
